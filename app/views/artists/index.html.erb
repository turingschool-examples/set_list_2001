<h1>All Artists</h1>

<section class = "artist-names-list">
  <% @artists.each do |artist| %>
    <section id = "artist-<%= artist.id %>" class = "individual-artist" >
      <%= artist.name %> <%= link_to "Delete", "/artists/#{artist.id}", method: :delete  %>
    </section>
  <% end %>
</section>

<%= link_to "New Artist", "/artists/new" %>
